<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>loot.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\cobblestone.jpg" />
    <None Remove="Images\loot.png" />
    <None Remove="Images\player1.png" />
    <None Remove="Images\player2.png" />
    <None Remove="Images\wall.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="loot.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\cobblestone.jpg" />
    <Resource Include="Images\loot.png" />
    <Resource Include="Images\player1.png" />
    <Resource Include="Images\player2.png" />
    <Resource Include="Images\wall.jpg" />
  </ItemGroup>

</Project>
